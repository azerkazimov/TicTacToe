{
    "builds": [
      {
        "src": "vite.config.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/main.jsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  